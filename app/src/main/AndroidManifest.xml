<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nisa.bismillah_dokter">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-permission android:name="android.permission.INTERNET" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" />
        <activity android:name=".sign.Log_in" />
        <activity android:name=".Splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sign.Sign_in" />
        <activity android:name=".sign.Sign_up" />
        <activity android:name=".tab_layout.list_item" />
        <activity android:name=".tab_layout.DETAIL" />
        <activity android:name=".persyaratan" />
        <activity android:name=".profile"></activity>
    </application>

</manifest>